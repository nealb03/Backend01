{
    "family": "allcloudsvcs-backend",
    "containerDefinitions": [
        {
            "name": "backend-container",
            "image": "288761763536.dkr.ecr.us-east-1.amazonaws.com/allcloudsvcs-backend:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/allcloudsvcs-backend",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "systemControls": []
        }
    ],
    "executionRoleArn": "arn:aws:iam::288761763536:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::288761763536:role/ecsTaskRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}
