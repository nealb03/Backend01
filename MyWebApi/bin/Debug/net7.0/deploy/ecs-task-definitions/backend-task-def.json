{
  "family": "brentaneal-backend",
  "executionRoleArn": "arn:aws:iam::288761763536:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "containerDefinitions": [
    {
      "name": "backend-container",
      "image": "288761763536.dkr.ecr.us-east-1.amazonaws.com/mywebapi:v9",
      "cpu": 512,
      "memory": 1024,
      "portMappings": [
        {
          "containerPort": 6000,
          "hostPort": 6000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "ConnectionStrings__DefaultConnection",
          "value": "server=cloud495.cmv0ysw00iqp.us-east-1.rds.amazonaws.com;port=3306;database=cloud495;user=admin;password=password"
        },
        {
          "name": "ASPNETCORE_URLS",
          "value": "http://0.0.0.0:6000"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/brentaneal-backend",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "backend"
        }
      }
    }
  ]
}